<div class="time-slot-container clearfix">
  <div class="item time-slot-unit">
    <div class="bfh-timepicker" data-icon="" data-time="<%= startTime %>" data-mode="12h"></div>
  </div>
  <div class="item time-slot-join">
    <span class="schedule-text-middle">to</span>
  </div>
  <div class="item time-slot-unit">
    <div class="bfh-timepicker" data-icon="" data-time="<%= endTime %>" data-mode="12h"></div>
  </div>
</div>