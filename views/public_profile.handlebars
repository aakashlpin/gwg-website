<img class="bg" src="/images/bg.jpg" alt=""/>
<nav class="navbar navbar-fixed-top navbar-profile" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <a class="navbar-brand" href="/">
        <img class="navbar-logo" src="/images/home-logo.png" alt="Guitar with Guru"/>
      </a>
    </div>
  </div>
</nav>
<div class="container" style="padding-top: 120px;">
  <div class="row mb-60">
    <div class="col-md-5 text-white">
      <div class="media mb-30">
        <div class="pull-left" style="margin-right: 30px;">
          <img class="media-object guru-image" src="{{picture}}" alt=""/>
        </div>
        <div class="media-body">
          <h2 class="guru-name">{{name}}</h2>
          <p class="guru-band">{{extras.band_name}}</p>
        </div>
      </div>

      <p class="guru-about-me">
        <i class="fa fa-quote-left" style="margin-right: 10px;"></i>
        {{extras.about_me}}
        <i class="fa fa-quote-right" style="margin-left: 10px;"></i>
      </p>
    </div>
    <div class="col-md-7">
      <div id="widget-schedule">
        <!-- space for schedule widget UI -->
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-4">
      <div id="widget-soundcloud">
        <!-- space for soundcloud widget UI -->
      </div>
    </div>
    <div class="col-md-8">
      <div id="widget-youtube">
        <!-- space for youtube widget UI -->
      </div>
    </div>
  </div>
</div>

<script src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.5.2/underscore-min.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.1.0/js/bootstrap.min.js"></script>
<script src="//connect.soundcloud.com/sdk.js"></script>
<script src="/bower_components/blueimp-gallery/js/jquery.blueimp-gallery.min.js"></script>
<script src="/bower_components/fullcalendar/fullcalendar.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/react/0.8.0/react.min.js"></script>
<script src="/dist/scripts/home/public_profile.js"></script>